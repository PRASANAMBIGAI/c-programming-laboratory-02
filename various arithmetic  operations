#include <stdio.h>
int main()
{
   int a,b,c,d,e,f,g;
   scanf("%d %d",&a,&b);
   c=a+b;
   d=a-b;
   e=a*b;
   f=a/b;
   g=a%b;
   printf("%d ·+·%d·=·%d",a,b,c);
   printf("\n%d ·-·%d·=·%d",a,b,d);
   printf("\n%d ·*·%d·=·%d",a,b,e);
   printf("\n%d ·/·%d·=·%d",a,b,f);
   printf("\n%d ·%·%d·=·%d",a,b,g);
   return 0;
}
